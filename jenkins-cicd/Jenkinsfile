pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                sh '''
                    echo 'Building..'
                    pwd
                    ls -la
              '''  
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
