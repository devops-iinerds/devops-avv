{
   "containerDefinitions": [ 
      { 
         "essential": true,
          "image": "250155391967.dkr.ecr.us-east-1.amazonaws.com/avv-jenkins-cdcd-demo:latest",
          "logConfiguration": { 
            "logDriver": "awslogs",
            "options": { 
               "awslogs-group" : "/ecs/avv-jenkins-cdcd-demo",
               "awslogs-region": "us-east-1",
               "awslogs-stream-prefix": "ecs"
            }
         },
         "name": "avv-jenkins-cdcd-demo",
         "entryPoint": [],
         "command": [],
         "portMappings": [ 
            { 
               "containerPort": 80,
               "hostPort": 80,
               "protocol": "tcp"
            }
         ]
      }
   ],
   "cpu": "256",
   "executionRoleArn": "arn:aws:iam::250155391967:role/ecsTaskExecutionRole",
   "family": "avv-jenkins-cdcd-demo",
   "memory": "512",
   "networkMode": "awsvpc",
   "runtimePlatform": {
        "operatingSystemFamily": "LINUX"
   },
   "requiresCompatibilities": [ 
       "FARGATE" 
    ]
}
